#!/usr/bin/env groovy

pipeline {
	agent any
	tools { 
        maven 'maven-3.5.0' 
        jdk 'JDK18' 
    }
	stages {
		stage('Build') {
			steps{
				echo 'This is where GW is created'
			}
		}
		stage('Setup') {
			steps{
				echo 'This is where GW is setup'
			}
		}
		stage('Test') {
			steps{
				sh "cd ci/Junit"
				sh "mvn test"
			}
		}
	}
}
