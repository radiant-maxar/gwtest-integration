#!/usr/bin/env groovy

pipeline {
	agent any
	tools { 
        maven 'Maven 4.11.0' 
        jdk 'jdk8' 
    }
	stages {
		stage('Build') {
			steps{
				echo 'This is where GW is created'
			}
		}
		stage('Setup') {
			steps{
				echo 'This is where GW is setup'
			}
		}
		stage('Test') {
			steps{
				cd 'ci/Junit'
				mvn test
			}
		}
	}
}
